     1                                  bits 32
     2                                  ; Calculati e - a * a
     3                                  global start        
     4                                  
     5                                  extern exit               
     6                                  import exit msvcrt.dll    
     7                                  
     8                                  segment data use32 class=data
     9 00000000 03                          a DB 3
    10 00000001 0800                        e DW 8
    11                                  
    12                                  segment code use32 class=code
    13                                      start:
    14 00000000 A0[00000000]                    MOV AL, [a]     ; AL = a
    15 00000005 F625[00000000]                  MUL byte[a]     ; AX := AL * a = a * a
    16 0000000B 668B15[01000000]                MOV DX, [e]     ; DX = e
    17 00000012 6629C2                          SUB DX, AX      ; DX = e - a * a
    18                                         
    19 00000015 6A00                            push    dword 0      
    20 00000017 FF15[00000000]                  call    [exit]       
